{
  "name": "@dicebear/schema",
  "version": "9.0.0-alpha.1",
  "description": "DiceBear JSON Schema generator",
  "keywords": [
    "avatar",
    "dicebear"
  ],
  "homepage": "https://www.dicebear.com",
  "bugs": {
    "url": "https://github.com/dicebear/dicebear/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dicebear/dicebear.git"
  },
  "license": "MIT",
  "author": "Florian KÃ¶rner <contact@dicebear.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "types": "./lib/index.d.ts",
    "default": "./lib/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "lib/index.d.ts"
      ]
    }
  },
  "files": [
